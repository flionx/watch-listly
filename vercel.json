{
  "version": 2,
  "builds": [
    {
      "src": "dist/client/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/tmdb.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/tmdb",
      "dest": "/api/tmdb"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
