{
  "version": 2,
  "buildCommand": "npm run build",
  "routes": [
    { "src": "/api/tmdb", "dest": "/api/tmdb.js" },
    { "src": "/library", "dest": "/dist/index.html" },
    { "src": "/friends", "dest": "/dist/index.html" },
    { "src": "/movie/.*", "dest": "/dist/index.html" },
    { "src": "/auth/.*", "dest": "/dist/index.html" },
    { "handle": "filesystem" },
    { "src": ".*", "dest": "/dist/index.html" }
  ]
}
