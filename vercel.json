{
  "version": 2,
  "builds": [
    {
      "src": "api/tmdb.ts",
      "use": "@vercel/node"
    },
    {
      "src": "client/index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/tmdb.ts?path=$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/index.html"
    }
  ]
}
